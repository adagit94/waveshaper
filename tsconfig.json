{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "useDefineForClassFields": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "jsx": "react",
    },
}
